{
  "name": "double.js",
  "version": "0.4.8",
  "description": "Floating point expansions with 31 accurate decimal digits (100+ bits), also known as double-word arithmetic",
  "main": "dist/double.js",
  "types": "dist/double.d.ts",
  "module": "dist/double.esm.js",
  "type": "module",
  "scripts": {
    "build": "rollup -c && asc wasm/mandel.ts -b wasm/mandel.wasm -O3 --runtime none",
    "test": "node --experimental-modules --no-warnings ./docs/puppeteer.js",
    "http": "http-server -c-1 -p 1234",
    "cov": "sed -i 's/.nyc_output\\/js/dist/g' .nyc_output/out.json && nyc report --reporter=lcov",
    "ci": "npm run http & (npm run test && npm run cov) || pkill http",
    "deploy": "npm version minor && git push --tags origin master"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "7.7.7",
    "assemblyscript": "^0.8.1",
    "codecov": "3.6.1",
    "http-server": "^0.12.0",
    "nyc": "^14.1.1",
    "puppeteer": "^2.0.0",
    "puppeteer-to-istanbul": "^1.2.2",
    "rollup": "^1.27.7",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-typescript2": "^0.25.2",
    "typescript": "^3.7.2",
    "zora": "^3.1.5"
  },
  "eslintIgnore": [
    "dist",
    "test"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "keywords": [
    "float128",
    "quadruple",
    "double-double",
    "gmp",
    "mpfr",
    "bigfloat",
    "bigdecimal",
    "bignumber.js",
    "decimal.js",
    "big.js"
  ],
  "author": "munrocket",
  "license": "MIT"
}
